

{{ include ('styleandjs.html.twig') }}
<div id="wrapper">
    <div class="vertical-align-wrap">
        <div class="vertical-align-middle">
            <div class="auth-box container-fluid">
                <div class=" right ">
                    <div class="overlay"></div>
                    <div class="content text">

                    </div>
                </div>
                <div class=" left ">
                    <div class="content py-5 ">
                       <div class="row">
                           <div  class="RegisterHeading">
                        <h3  >Rregjistrohu</h3>
                           <p class="lead">Nese keni llogari,<a href="{{ path('homepage') }}"> identifikohuni!</a></p>


                           </div>
                           <div class="header"  style="float:right">
                               <div class="logo text-center"><img src="assets/img/Logo.png"  alt=" Logo" width="100px"></div>


                           </div>
                       </div>
                        <div class="row">
                        {{ form_start(form) }}

                            <div class="form-group col-md-6">
                                {{ form_row(form.emer_biznesi,{'attr': {'class': 'form-control'}}) }}

                            </div>
                            <div class="form-group col-md-6">
                                {{ form_row(form.nipt,{'attr': {'class': 'form-control','for':'Emerbiznesi'}}) }}
                            </div>
                        </div>
                        <div class="row">
                            <div class="form-group  col-md-6 ">
                            {{ form_row(form.email,{'attr': {'class': 'form-control'}}) }}
                            </div>
                            <div class="form-group col-md-6">
                                {{ form_row(form.numerTelefoni,{'attr': {'class': 'form-control'}}) }}
                            </div>
                        </div>
<hr>
                        <div class="row">
                            <div class="form-group  col-md-6 ">
                                {{ form_row(form.fushe_operimi_id,{'attr': {'class': 'form-control'}}) }}

                            </div>
                            <div class="form-group col-md-6">
                                {{ form_row(form.bashkia,{'attr': {'class': 'form-control'}}) }}

                            </div>
                        </div>
                        <div class="row">
                            <div class="form-group  col-md-12 ">
                                {{ form_row(form.adresa,{'attr': {'class': 'form-control'}})}}
                            </div>
                        </div>
                        <hr>
                        <div class="row">
                            <div class="form-group  col-md-6 ">
                                {{ form_row(form.logo,{'attr': {'class': 'form-control'}}) }}
                            </div>
                            <div class="form-group  col-md-6 ">
                                {{ form_row(form.password,{'attr': {'class': 'form-control'}}) }}
                            </div>
                        </div>



                        {{ form_row(form.submit,{'attr': {'class': 'btn btn-primary btn-lg btn-block',
                            'tag':'button','style':'width: 100%; padding-left:0px;background-color:#FFC107'}})}}
                        {{ form_end(form) }}
                        </div>


                    </div>
                </div>

                <div class="clearfix"></div>
            </div>
        </div>
    </div>

</div>

